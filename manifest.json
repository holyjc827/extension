{
    "manifest_version" : 2,
    "name": "Break Reminder",
    "description": "a",
    "version": "1.0",
    "permissions": [
     "activeTab","storage","notifications","alarms"
     ],
    "background":{
      "scripts":["background.js"],
      "persistent": true
    },
    "browser_action": {
//     "default_icon": "icon.png",
     "default_popup": "templates/popup.html"
    },
    "content_scripts": [
        {
            "js": [ "jquery-3.5.0.min.js", "popup.js" ],
            "matches": [ "http://*/*", "https://*/*"]
        }
    ]

}